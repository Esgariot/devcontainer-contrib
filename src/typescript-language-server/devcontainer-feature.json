{
    "name": "typescript language server",
    "id": "typescript-language-server",
    "version": "1.0.0",
    "description": "A feature to install typescript language server",
    "options": {
        "version": {
            "type": "string",
            "proposals": ["latest"],
            "default": "latest"
        },
        "typescriptVersion": {
            "type": "string",
            "proposals": ["latest", "none"],
            "default": "latest"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/node"
    ]
}
